{
  "name": "markdown-it-table-copy",
  "private": false,
  "version": "1.0.3",
  "description": "A markdown-it plugin to add a copy icon bottom of the table to copy the table to clipboard.",
  "homepage": "https://github.com/Miura-KR/markdown-it-table-copy",
  "bugs": {
    "url": "https://github.com/Miura-KR/markdown-it-table-copy/issues"
  },
  "author": "kotaro-miura",
  "license": "MIT",
  "type": "module",
  "files": [
    "dist"
  ],
  "keywords": [
    "markdown",
    "markdown-it",
    "markdown-it-plugin",
    "plugin",
    "table",
    "copy"
  ],
  "repository": "Miura-KR/markdown-it-table-copy",
  "main": "./dist/index.umd.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.umd.cjs"
    }
  },
  "scripts": {
    "dev": "bunx --bun vite",
    "build": "bun run lint && bunx --bun vite build",
    "test": "vitest",
    "lint": "eslint lib/**/*.ts src/**/*.ts test/**/*.ts"
  },
  "devDependencies": {
    "@eslint/js": "^9.24.0",
    "@mdi/font": "^7.4.47",
    "@types/jsdom": "^27.0.0",
    "@types/markdown-it": "^14.1.2",
    "eslint": "^9.24.0",
    "jsdom": "^28.0.0",
    "markdown-it": "^14.1.0",
    "markdown-it-table-copy": "^1.0.3",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.29.0",
    "vite": "^7.0.0",
    "vite-plugin-checker": "^0.12.0",
    "vite-plugin-dts": "^4.5.3",
    "vitest": "^4.0.3"
  },
  "dependencies": {
    "clipboard": "^2.0.11"
  }
}
